generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:../config/db.sqlite"
}

enum Role {
  ADMIN
  USER
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  name     String
  password String
  role     Role   @default(USER)
}
